{
    "name": "@echo/<%=apiName%>",
    "version": "1.0.0",
    "description": "",
    "keywords": [],
    "author": "",
    "license": "ISC",
    "main": ".dist/index.js",
    "typings": ".dist/index.d.ts",
    "scripts": {
        "build:clean": "rm -rf .dist && rm -rf ./src/_generated/*",
        "build:generate:domain:code": "pnpm exec builder api domain generate",
        "build:generate:rest:schema": "pnpm exec builder api rest generate-schema",
        "build:generate:rest:api": "pnpm exec builder api rest generate-api",
        "build:generate": "pnpm build:generate:domain:code && pnpm build:generate:rest:schema && pnpm build:generate:rest:api",
        "build:compile": "tsc && tsc-alias",
        "build:post": "cp src/_generated/rest.schema.generated.json .dist/_generated",
        "build": "pnpm build:clean && pnpm build:generate && pnpm build:compile && pnpm build:post",
        "package:make": "rush deploy --target-folder .deploy --overwrite",
        "package:container": "docker build . --tag <%=apiName%>:1.0.0",
        "package": "pnpm package:make && pnpm package:container",
        "test": "jest",
        "devcontainer:start": "docker run --env-file .env --workdir /app/api/<%=apiName%> <%=apiName%>:1.0.0 npm run dev:start",
        "dev:start": "node --inspect=0.0.0.0:9229 .",
        "dev": "pnpm build && pnpm dev:start",
        "start": "node ."
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.3",
        "@types/luxon": "^3.3.0",
        "@types/node": "^20.4.2",
        "@types/uuid": "^9.0.2",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-jest": "^27.2.3",
        "jest": "^29.6.1",
        "prettier": "^3.0.0",
        "ts-jest": "^29.1.1",
        "typescript": "^5.1.6"
    },
    "dependencies": {
        "@echo/builder": "workspace:^1.0.0",
        "@echo/lib-common": "workspace:^1.0.0",
        "@echo/lib-domain-api": "workspace:^1.0.0",
        "@eventstore/db-client": "^5.0.1",
        "express": "^4.18.2",
        "luxon": "^3.3.0",
        "openapi-backend": "^5.9.2",
        "source-map-support": "^0.5.21",
        "tsc-alias": "^1.8.7",
        "uuid": "^9.0.0",
        "zod": "^3.22.2",
        "dotenv": "~16.3.1"
    }
}
