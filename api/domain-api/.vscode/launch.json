{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "local:launch",
            "type": "node",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/.build/index.js",
        },
        {
            "name": "docker:attach",
            "type": "node",
            "restart": true,
            "request": "attach",
            "port": 4101,
            "address": "localhost",
            "remoteRoot": "/app/api/domain-api",
            "localRoot": "${workspaceFolder}",
            "skipFiles": [
                "<node_internals>/**",
                "node_modules/**"
            ]
        }
    ]
}